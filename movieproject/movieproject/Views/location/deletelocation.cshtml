@model movieproject.Models.tbllocation

@{
    ViewBag.Title = "deletelocation";
}

<h2>deletelocation</h2>

<h3>Are you sure you want to delete this?</h3>
<div class="row">
    <div class="col-md-12">
        <h4>mobile details</h4>
        <hr />

        @using (Html.BeginForm())
        {
            <div class="form-group">
                <label for="mobile">Location</label>
                @Html.TextBoxFor(obj => obj.mocation, new { @class = "form-control" })
            </div>

            <input type="submit" name="name" value="delete" class="btn btn-success" />

        }
    </div>
</div>

    

