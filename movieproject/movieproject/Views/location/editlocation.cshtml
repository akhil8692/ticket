@model movieproject.Models.tbllocation

@{
    ViewBag.Title = "editlocation";
}

<h2>editlocation</h2>

<div class="row">
    <div class="col-md-12">
        <h4>mobile details</h4>
        <hr />

        @using (Html.BeginForm())
        {
            <div class="form-group">
                <label for="mobile">Location</label>
                @Html.TextBoxFor(obj => obj.locid,new { @class="form-control"})
                @Html.TextBoxFor(obj => obj.mocation, new { @class = "form-control" })
            </div>

            <input type="submit" name="name" value="edit" class="btn btn-success" />

        }
    </div>
</div>


